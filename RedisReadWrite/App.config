<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <!--Redis写入服务器地址,可以添加多个服务器通过,分隔-->
    <add key="ReadWriteHosts" value="192.168.1.88:6379" />
    <!--Redis读服务器地址,可以添加多个服务器通过,分隔-->
    <add key="ReadOnlyHosts" value="192.168.1.88:6380,127.0.0.1:6379" />
  </appSettings>
</configuration>